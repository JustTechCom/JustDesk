{
  "root": true,
  "extends": ["next/core-web-vitals"],
  "rules": {
    "@next/next/no-html-link-for-pages": "off",
    "react/jsx-no-undef": "off",
    "react-hooks/exhaustive-deps": "off",
    "import/no-anonymous-default-export": "off",
    "react/no-unescaped-entities": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "prefer-const": "off",
    "eqeqeq": "off"
  }
}
